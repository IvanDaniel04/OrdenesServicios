<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="610.0" prefWidth="1080.0"
            xmlns="http://javafx.com/javafx/17" fx:controller="jin.com.edu.ordenesservicios.serviciosGenController">
    <children>
        <ImageView fitHeight="610.0" fitWidth="1080.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../solicitudes.png"/>
            </image>
        </ImageView>
        <Button layoutX="812.0" layoutY="164.0" mnemonicParsing="false" text="🔍">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <TableView fx:id="tblServcios" layoutX="38.0" layoutY="215.0" onMouseClicked="#DobleClick" prefHeight="291.0"
                   prefWidth="1009.0">
            <columns>
                <TableColumn fx:id="clmId" prefWidth="73.59997177124023" text="N. Registro"/>
                <TableColumn fx:id="clmFecha" prefWidth="88.80001831054688" text="Fecha"/>
                <TableColumn fx:id="clmDescripcion" prefWidth="324.6241455078125" text="Descripción"/>
                <TableColumn fx:id="clmArea" prefWidth="137.87591552734375" text="Area solicitante"/>
                <TableColumn fx:id="clmNombreS" prefWidth="177.97979736328125" text="Nombre solicitante"/>
                <TableColumn fx:id="clmUbicacion" minWidth="0.0" prefWidth="204.5201416015625" text="Ubicacion"/>
            </columns>
        </TableView>
        <TextField fx:id="txtBuscar" layoutX="225.0" layoutY="164.0" onKeyTyped="#Buscar" prefHeight="30.0"
                   prefWidth="590.0"/>
        <ImageView fitHeight="46.0" fitWidth="52.0" layoutX="1014.0" layoutY="79.0" onMousePressed="#pane"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../user.png"/>
            </image>
        </ImageView>
        <Pane fx:id="paneSerGen" layoutX="850.0" layoutY="130.0" prefHeight="143.0" prefWidth="200.0"
              style="-fx-background-color: #F8F8FF#F8F8FF; -fx-border-color: #00008B#00008B;" visible="false">
            <children>
                <Button layoutX="13.0" layoutY="38.0" mnemonicParsing="false" onAction="#registarUsuario"
                        prefHeight="28.0" prefWidth="68.0"
                        style="-fx-background-color: #DCDCDC#DCDCDC; -fx-background-radius: 6; -fx-border-color: #00008B #00008B; -fx-border-radius: 6;"
                        text="REGISTRAR">
                    <font>
                        <Font name="Bodoni 72 Book" size="10.0"/>
                    </font>
                </Button>
                <Button layoutX="13.0" layoutY="88.0" mnemonicParsing="false" onAction="#cerrarSesión" prefHeight="22.0"
                        prefWidth="178.0"
                        style="-fx-background-color: #DCDCDC#DCDCDC; -fx-background-radius: 6; -fx-border-color: #00008B #00008B; -fx-border-radius: 6;"
                        text="CERRAR SESIÓN">
                    <font>
                        <Font name="Bodoni 72 Oldstyle Book" size="13.0"/>
                    </font>
                </Button>
                <Button layoutX="111.0" layoutY="38.0" mnemonicParsing="false" onAction="#Vusuarios" prefHeight="28.0"
                        prefWidth="68.0"
                        style="-fx-background-color: #DCDCDC#DCDCDC; -fx-background-radius: 6; -fx-border-color: #00008B #00008B; -fx-border-radius: 6;"
                        text="USUARIOS">
                    <font>
                        <Font name="Bodoni 72 Book" size="10.0"/>
                    </font>
                </Button>
            </children>
        </Pane>

    </children>
</AnchorPane>
